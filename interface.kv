<WindowChooseRole>:
    name: 'choose_role'
    title: 'Вход'
    size:root.width, root.height

    BoxLayout:
        background_color:'#D1BF8D'
        background_normal: ''
        canvas.before:
            Color:
                rgb: (238/256,230/256,203/256,1)
            Rectangle:
                pos: self.pos
                size: self.size

        orientation: "vertical"
        adaptive_size: True

        GridLayout:
            cols:1
            height:50
            padding:15,120,10,0
            Label:
                text:"[color=000000]Войти как[/color]"
                markup: True
                font_size: 20
        GridLayout:
            cols:2
            spacing:20
            height:70
            padding:300,0,10,0
            MDRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: 'Читатель'
                on_press:
#                    app.root.transition.direction = 'no_transition'
                    app.root.current = 'choose_reader'
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1
            MDRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: 'Работник'
                #disabled: True if root.everything() else False
                on_press:
#                    app.root.transition.direction = 'no_transition'
                    app.root.current = 'choose_worker'
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1



<WindowWelcomeReader>:
    name: 'choose_reader'
    title: 'Вход'
    separator_color: (192/256,110/256,240/256,1)
    background_color:'#a910e6'
    size:root.width, root.height

    BoxLayout:
        # background color
        canvas.before:
            Color:
                rgb:(238/256,230/256,203/256,1)
            Rectangle:
                pos: self.pos
                size: self.size



        orientation: "vertical"
        adaptive_size: True

        GridLayout:
            cols:1
            height:50
            padding:15,140,10,0
            Label:
                text:"[color=000000]Вы хотите?[/color]"
                markup: True
                font_size: 20
                height:30
                width:30
        GridLayout:
            cols:1
            rows:2
            spacing:20
            height:70
            padding:320,0,10,0
            MDFillRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: '                Войти                 '
                #disabled: True if root.everything() else False
                on_press:
#                    app.root.transition.direction = 'no_transition'
                    app.root.current = 'reader_enter'
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}
            MDFillRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: '  Зарегистрироваться'
                #disabled: True if root.everything() else False
                on_press:
                    app.root.current = 'reader_registration'
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}

<WindowEnterReader>:
    name: 'reader_enter'
    title: 'Вход'
    size:root.width, root.height

    BoxLayout:
        # background color
        canvas.before:
            Color:
                rgb: (238/256,230/256,203/256,1)
            Rectangle:
                pos: self.pos
                size: self.size



        orientation: "vertical"
        adaptive_size: True
        GridLayout:
            cols:1
            height:50
            padding:30,140,10,0
            Label:
                text:"[color=000000]Добро пожаловать![/color]"
                markup: True
                font_size: 20

        GridLayout:
            cols:1
            rows:1
            height:30
            padding:310,40,0,0
        # имя пациента
            MyField
                id: login_reader
                hint_text: 'Номер чит-го билета'
                max_text_len: 20
                on_text: root.set_login(self.text)
                pos_hint: {"center_x": .30}
        GridLayout:
            cols:2
            rows:1
            padding:310,10,10,50

            MyField
                id: password
                hint_text: 'Пароль'
                max_text_len: 20
                on_text: root.set_password(self.text)
                pos_hint: {"center_x": .30}

        GridLayout:
            cols:2
            rows:1
            spacing:10
            padding:300,20,0,0
            MDFillRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: '      Войти       '
                #disabled: True if root.everything() else False
                on_press: app.root.current = 'reader_menu'
                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}

            MDFillRoundFlatButton:
                md_bg_color: 1, 1, 1,1
                text: '    Отмена    '
                #disabled: True if root.everything() else False
                on_press: app.root.current = 'choose_reader'

                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 1, 1, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}



